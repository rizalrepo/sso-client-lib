{
    "name": "rizalrepo/sso-client",
    "description": "SSO Client OAuth 2.0",
    "type": "laravel-package",
    "version": "1.0.0",
    "require": {
        "illuminate/support": "^11.1",
        "php": "^7.4|^8.0",
        "rizalrepo/sso-client": "^1.0"
    },
    "require-dev": {
        "illuminate/support": "^11.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rizalrepo\\SsoClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "rizalrepo",
            "email": "rizalyusei@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Rizalrepo\\SsoClient\\SSOClientServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rizalrepo/sso-client"
        }
    ]
}
