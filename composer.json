{
    "name": "rizalrepo/laravel-sso-client",
    "description": "Laravel SSO Client Library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "rizalrepo/sso-client": "1.0.0@dev"
    },
    "autoload": {
        "psr-4": {
            "rizalrepo\\LaravelSSOClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rizalrepo\\LaravelSSOClient\\SSOClientServiceProvider"
            ]
        }
    },
    "config": {
        "minimum-stability": "dev"
    }
}
